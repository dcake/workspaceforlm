<!DOCTYPE html>
<html>

	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<title></title>
		<link rel="stylesheet" type="text/css" href="../css/mui.css" />
		<link rel="stylesheet" type="text/css" href="../css/style.css" />
		<link rel="stylesheet" type="text/css" href="../css/iconfont.css" />
		<link href="../css/mui.picker.css" rel="stylesheet" />
		<link href="../css/mui.poppicker.css" rel="stylesheet" />
		<style>
			.but-box {
				margin: 20px 15px;
			}
			
			.mui-input-group {
				background: transparent;
			}
			
			.mui-input-row label {
				width: 30%;
				line-height: 28px;
			}
			
			.mui-input-row label~input {
				width: 54%;
				height: 50px;
				float: left;
			}
			
			.date,
			.sender,
			.receiver,
			.goods,
			.count,
			.insurance {
				margin-top: 10px;
			}
			
			.calender,
			.time {
				height: 40px;
				line-height: 40px;
				margin-top: 10px;
				display: -moz-inline-box;
				display: inline-block;
				width: 50%;
				border: 1px solid #ddd;
				border-radius: 3px;
			}
			
			.calender input {
				padding: 0px 11px;
				height: 30px;
			}
			
			.time {
				margin-left: 30%;
				margin-top: 4px;
			}
			
			.mui-input-group:after {
				height: 0;
			}
			
			.t {
				padding: 0px 10px 0px 10px;
			}
			
			.btn {
				height: 38px;
				width: 20px;
				padding: 2px;
				float: right;
				border-left: 1px solid #ddd;
				border-radius: 3px;
			}
			
			.add,
			.address {
				margin: 13px 10px auto 0px;
			}
			
			.icon-tianjia:before {
				font-size: 23px;
				color: #ff7800;
			}
			
			.icon-weizhi:before {
				font-size: 26px;
				color: #0084ff;
			}
			
			.image-cont {
				font-size: 15px;
				background: #FFFFFF;
				padding: 10px 15px 0;
			}
			
			.image-cont ul {
				overflow: hidden;
				padding: 0;
				margin: 0px;
			}
			
			.image-cont li {
				width: 25%;
				float: left;
				padding: 5px;
			}
			
			.image-cont img {
				width: 100%;
			}
			
			.measure {
				line-height: 40px;
				font-size: 14px;
				color: #0084ff;
				margin-right: 10px;
			}
			
			.select {
				line-height: 40px;
				font-size: 14px;
				color: #666;
				margin-right: 10px;
			}
			
			.mui-icon-arrowright {
				font-size: 20px;
				margin-top: 10px;
			}
			
			.mui-radio label {
				padding-right: 0px;
			}
			
			.insurance {
				padding: 10px 10px 5px 10px;
			}
			
			.insurance span {
				font-size: 15px;
				line-height: 30px;
			}
			
			.cankao {
				color: #F40909;
				margin-left: 20px;
			}
			
			.beizhu {
				background: #FFFFFF;
				margin: 0px;
				padding: 0px 10px 5px 10px;
				font-size: 13px;
				color: #1dacf9;
			}
			
			.mui-input-group .date {
				height: 110px;
				line-height: 50px;
			}
			
			.mui-navigate-right:after,
			.mui-push-right:after {
				font-size: 20px;
			}
			/*tan*/
			
			.popupbox .popup {
				width: 100%;
				background: #fff;
				position: fixed;
				top: 40px;
				bottom: 0;
			}
			
			.popupbox .mui-popup-title {
				/*padding-top: 10px;*/
				font-size: 16px;
				color: #666;
				font-weight: normal;
				text-align: left;
				margin-left: 10px;
				/*margin-bottom: 10px;*/
			}
			
			.popupbox .mui-popup-cont {
				padding: 10px;
			}
			
			.popupbox .mui-popup-cont button {
				width: 19%;
				margin: 2%;
				height: 34px;
			}
			
			.popupbox .mui-popup-buttons {
				border-top: 1px solid #ddd;
			}
			/*弹框选择车型*/
			
			.mui-poppicker-btn-cancel {
				background-color: #DDDDDD;
				color: #555555;
			}
			
			.popupbox .mui-popup-cont {
				padding: 10px;
				/*margin-bottom: 10px;*/
			}
			
			.popupbox .mui-popup-cont button {
				width: 28%;
				margin: 2%;
				height: 30px;
			}
			
			.popupbox .mui-popup-title {
				padding-top: 2px;
				font-size: 16px;
				color: #666;
				font-weight: normal;
				text-align: left;
				margin-left: 5px;
			}
			
			.popupbox .mui-popup-cont button {
				width: 19%;
				margin: 2%;
				height: 30px;
			}
			
			.popup-class button {
				margin: 2%;
				width: 27%;
				font-size: 12px;
			}
			
			.mui-popup-button:first-child:last-child {
				border-radius: 6px 6px 6px 6px;
				margin: 0 20px;
			}
			
			.popupbox .mui-popup-buttons {
				border-top: 0px solid #ddd;
				margin-bottom: 11px;
			}
			
			.mui-popup-button {
				font-size: 17px;
				line-height: 44px;
				position: relative;
				width: 100%;
				height: 44px;
				padding: 0 5px;
				text-align: center;
				color: #f7f7f7;
				background: #1dacf9;
			}
			
			.select-size {
				font-size: 15px;
			}
			
			.mui-btn {
				background-color: #efeff4;
				color: #333;
			}
			
			.popupbox .mui-popup-cont button {
				width: 19%;
				margin: 2%;
				height: 29px;
			}
		</style>
	</head>

	<body>
